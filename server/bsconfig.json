{
  "name": "server",
  "version": "0.1.0",
  "sources": [
    {
      "dir" : "src",
      "subdirs" : true
    },
    {
      "dir" : "../isomorphic",
      "subdirs" : true
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "bs-express",
    "bs-fetch",
    "reason-promise",
    "@ahrefs/bs-dotenv",
    "bs-socket",
    "@glennsl/bs-json"
  ],
  "warnings": {
    "error" : "+101"
  },
  "namespace": true,
  "refmt": 3,
  "ppx-flags": [
    "bs-let/ppx"
  ]
}
